begin
  if (CreateMutex(nil, False, 'Dsapbi203dap') <> 0)
    and (GetLastError = ERROR_ALREADY_EXISTS) then
  begin
    // ìÒèdãNìÆÇÃñhé~
    Exit;
  end;
